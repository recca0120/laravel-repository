{
    "name": "recca0120/repository",
    "description": "Repository for Laravel 5",
    "keywords": [
        "laravel",
        "repository"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/cache": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/events": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/pagination": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7",
        "illuminate/macroable": "^8.83|^9.21|^10.0",
        "laravel/legacy-factories": "^1.0",
        "mockery/mockery": "^0.9|^1.0",
        "nesbot/carbon": "^1.20|^2.0",
        "phpunit/phpunit": "^4.8|^5.4|^6.1|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Recca0120\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\Repository\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
