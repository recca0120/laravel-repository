{
    "name": "recca0120/repository",
    "description": "Repository for Laravel 5",
    "keywords": ["laravel", "repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/pagination": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8|~5.4|~6.1",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\Repository\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
